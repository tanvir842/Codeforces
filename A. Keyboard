#include<bits/stdc++.h>
using namespace std;
int main(){
    char a;
    string s,p;
    cin>>a>>s;
    p="qwertyuiopasdfghjkl;zxcvbnm,./";
    if(a=='R'){
        for (int i = 0; i < s.size(); i++)
        {
           for (int j = 0; j < p.size(); j++)
           {
             if(s[i]==p[j]){
                cout<<p[j-1];
             }
           }
            
        }
        
    }
    else if(a=='L'){
        for (int i = 0; i < s.size(); i++)
        {
           for (int j = 0; j < p.size(); j++)
           {
             if(s[i]==p[j]){
                cout<<p[j+1];
             }
           }
            
        }
        
    }
}
